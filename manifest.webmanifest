{
  "id": "com.moneymanager.app",
  "name": "Money Manager",
  "short_name": "MoneyManager",
  "description": "Track your expenses, manage budgets, and achieve financial goals with our comprehensive money management app",
  "theme_color": "#dbe9fe",
  "background_color": "#fafafa",
  "display": "standalone",
  "display_override": ["window-controls-overlay", "standalone", "minimal-ui"],
  "orientation": "portrait-primary",
  "scope": "./",
  "start_url": "./",
  "categories": ["finance", "productivity", "utilities"],
  "lang": "en",
  "dir": "ltr",
  "prefer_related_applications": false,
  "edge_side_panel": {
    "preferred_width": 400
  },
  "launch_handlers": {
    "client_mode": "navigate-existing",
    "route_to": "existing-client",
    "exclude_matches": [
      {
        "url_pattern": "https://admin.prashiln79.github.io/*"
      },
      {
        "url_pattern": "https://support.prashiln79.github.io/*"
      }
    ]
  },
  "file_handlers": [
    {
      "action": "./",
      "accept": {
        "text/csv": [".csv"],
        "application/vnd.ms-excel": [".xls"],
        "application/vnd.openxmlformats-officedocument.spreadsheetml.sheet": [".xlsx"],
        "application/pdf": [".pdf"],
        "text/plain": [".txt"]
      }
    }
  ],
  "handle_links": "preferred",
  "protocol_handlers": [
    {
      "protocol": "web+money",
      "url": "./?protocol=%s"
    },
    {
      "protocol": "web+transaction",
      "url": "./?action=add-transaction&data=%s"
    },
    {
      "protocol": "web+budget",
      "url": "./?action=budget&id=%s"
    }
  ],
  "share_target": {
    "action": "./?action=share",
    "method": "POST",
    "enctype": "multipart/form-data",
    "params": {
      "title": "title",
      "text": "text",
      "url": "url",
      "files": [
        {
          "name": "receipts",
          "accept": ["image/*", "application/pdf", "text/csv"]
        }
      ]
    }
  },
  "icons": [
